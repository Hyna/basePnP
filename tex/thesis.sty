%% Vychozi text prohlaseni v pouzitych jazycich
\newcommand\prohlasenitext@czechvut%
{%
  \par%\hspace{2ex}
  Prohla\v{s}uji, \v{z}e 
  \ifx\akt@type\type@semestral
    sv\r{u}j semestr\'{a}ln\'{\i} projekt
  \fi
  \ifx\akt@type\type@bachelor
    svou bakal\'{a}\v{r}skou pr\'{a}ci
  \fi
  \ifx\akt@type\type@diploma
    svou diplomovou pr\'{a}ci
  \fi
  \ifx\akt@type\type@phd
    svou doktorskou pr\'{a}ci
  \fi
  \ifx\akt@type\type@treatise
    sv\'{e} pojedn\'{a}n\'{i} o dizerta\v{c}n\'{i} pr\'{a}ci
  \fi
  na t\'{e}ma \uv{\nazev@czechvut} jsem vypracoval(a) samostatn\v{e} pod veden\'{\i}m vedouc\'{\i}ho 
  \ifx\akt@type\type@semestral
    semestr\'{a}ln\'{\i}ho projektu
  \fi
  \ifx\akt@type\type@bachelor
    bakal\'{a}\v{r}sk\'{e} pr\'{a}ce
  \fi
  \ifx\akt@type\type@diploma
    diplomov\'{e} pr\'{a}ce
  \fi
  \ifx\akt@type\type@phd
    doktorsk\'{e} pr\'{a}ce
