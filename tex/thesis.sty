%% Vysazeni seznamu obrazku
\newcommand{\seznamobrazku}%
  {{% prostredi kvuli \pagestyle
    \pagestyle{empty}
    \cleardoublepage
    \listoffigures
    \newpage
	
    \if@twoside
      \pagestyle{empty}
      \cleardoublepage
    \fi
  }}%


%% Vysazeni seznamu tabulek
\newcommand{\seznamtabulek}%
  {{% prostredi kvuli \pagestyle
    \pagestyle{empty}
    \cleardoublepage
